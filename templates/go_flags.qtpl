{% func GoGenPflags(pkg string, fds []map[string]interface{}) -%}
// Code generated by web-cli. DO NOT EDIT.

package {%s pkg %}

func init() {
{% for _, fd := range fds -%}
{% stripspace %}
	{%s fd["FlagType"].(string) %}("{%j fd["Name"].(string) %}",
	 {%s= fd["DefaultValue"].(string) %},
	 {% code
			help := fd["Help"].(string)
			if fd["Required"].(bool){
				help += " (required)"
			}
	 %}
	 "{%s= help %}")
{% endstripspace %}
{% endfor -%}
}
{% endfunc %}